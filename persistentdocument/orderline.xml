		<add name="unitPriceWithTax" type="Double" min-occurs="1" />
		<add name="unitPriceWithoutTax" type="Double" min-occurs="1" />
		<add name="quantity" type="Double" min-occurs="1" />
		<add name="amountWithTax" type="Double" min-occurs="1" />
		<add name="amountWithoutTax" type="Double" min-occurs="1" />

		<add name="globalProperties" type="Lob" />
