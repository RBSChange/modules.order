	xsi:schemaLocation="http://www.rbs.fr/schema/change-document/1.0 http://www.rbschange.fr/static/schema/change-document/1.0.xsd"
	model-version="1.0" icon="cart-modifier" publish-on-day-change="true" has-url="false">
	<properties>
		<add name="shop" type="modules_catalog/shop" min-occurs="1" inverse="true" from-list="modules_catalog/shops" />
		<add name="exclusive" type="Boolean" default-value="false" />
		<add name="excludeDiscount" type="modules_order/cartmodifier" max-occurs="-1" />
		<add name="description" type="XHTMLFragment" />
	</properties>
	<statuses default="DRAFT" />
	<serializedproperties />
</document>